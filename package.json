{
    "name": "banking-fraud-detection",
    "version": "1.0.0",
    "description": "Production-grade fraud detection microservice for banking transactions",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "test": "jest --coverage",
        "test:unit": "jest --testPathPattern=unit",
        "test:integration": "jest --testPathPattern=integration",
        "lint": "eslint src/**/*.ts",
        "typecheck": "tsc --noEmit",
        "migrate": "sequelize-cli db:migrate",
        "migrate:undo": "sequelize-cli db:migrate:undo"
    },
    "dependencies": {
        "@tensorflow/tfjs-node": "^4.17.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "geoip-lite": "^1.4.9",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "joi": "^17.11.0",
        "kafkajs": "^2.2.4",
        "onnxruntime-node": "^1.17.0",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.35.2",
        "ua-parser-js": "^1.0.37",
        "uuid": "^9.0.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/geoip-lite": "^1.4.4",
        "@types/jest": "^29.5.11",
        "@types/node": "^20.10.6",
        "@types/ua-parser-js": "^0.7.39",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.9.3"
    },
    "engines": {
        "node": ">=18.0.0"
    },
    "license": "UNLICENSED",
    "private": true
}